{
  "parserOptions": {
    "createDefaultProgram": true,
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "no-eval": 2,
    "prefer-const": 2
  }
}
